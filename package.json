{
  "name": "quipuswap",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./build",
    "migrate": "yarn clean && truffle migrate",
    "migrate-granadanet": "yarn clean && truffle migrate --network granadanet",
    "migrate-granadanet-reset": "yarn clean && truffle migrate --network granadanet --reset",
    "compile-dex": "ligo compile contract ./contracts/main/DexFA12.ligo -e main --format json > ./contracts/main/DexFA12.tz",
    "compile-scorer": "ligo compile contract ./contracts/main/gamifications/Scorer.ligo -e main --format json > ./contracts/main/gamifications/Scorer.tz",
    "compile-token": "ligo compile contract ./contracts/main/gamifications/ScoreFA12.ligo -e main --format json > ./contracts/main/gamifications/ScoreFA12.tz && ligo compile contract ./contracts/main/gamifications/DummyFA12.ligo -e main --format json > ./contracts/main/gamifications/DummyFA12.tz",
    "compile": "yarn compile-dex && yarn compile-token && yarn compile-scorer",
    "start-sandbox": "ganache-cli --flavor tezos --seed alice --hardfork edo",
    "test": "ts-mocha",
    "test:quick": "npm test",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@taquito/signer": "^8.0.4-beta.0",
    "@taquito/taquito": "^11.0.1",
    "@taquito/tezbridge-signer": "^11.0.1",
    "bignumber.js": "^9.0.0",
    "commander": "^5.1.0",
    "dotenv": "^8.2.0",
    "ganache-cli": "6.12.1-tezos.0",
    "patch-package": "^6.2.2",
    "truffle": "5.5.0-tezos.4",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.0.3"
  },
  "directories": {
    "test": "./test"
  },
  "devDependencies": {
    "@types/chai": "^4.2.13",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "ts-node": "^9.0.0"
  }
}
